generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}
        
model AbResult {
  id         Int      @id @default(autoincrement())
  testName   String
  variant    String
  event      String
  visitorId  String
  createdAt  DateTime @default(now())

  @@index([testName, variant])
  @@index([visitorId])
}
