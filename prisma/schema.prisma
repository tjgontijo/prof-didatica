generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}
        
model AbResult {
  id         String   @id @default(cuid())
  testName   String
  variant    String
  event      String
  visitorId  String
  createdAt  DateTime @default(now())

  @@index([testName, variant])
  @@index([visitorId])
}

model User {
  id        String   @id @default(cuid())
  nome      String
  phone     String
  messageId String
  sessionId String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([phone])
  @@index([sessionId])
}
